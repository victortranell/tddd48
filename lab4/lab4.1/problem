(defproblem problem domain
    ;;init
    (
        (location depot)
        (location loc1)

        (content food)
        (content medicin)

        (at depot crate1)
        (has crate1 food)
        (at depot uav1)
        (uav uav1)
        (free uav1)

        (person person1)
        (at loc1 person1)
        (needs person1 food)
    )
    (
        ;;goal
        (deliver-all)
    )
)